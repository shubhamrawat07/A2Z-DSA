vector<long long> lcmAndGcd(long long A , long long B) {
        // code here
        long long gcd = 1, lcm = 1, a = A, b = B;
        
        while(a && b){
            if(a > b)
                a = a % b;
            else
                b =  b % a;
        }
        if(a == 0) gcd = b;
        else gcd = a;
        
        lcm = (A*B)/gcd;
        return {lcm, gcd};
    }
